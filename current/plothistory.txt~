run ~/py/mread/__init__.py
dstress=np.load('diskstress_1600norm.npy') #disk values
cstress=np.load('coronastress_1600norm.npy') #corona values
bstress=np.load('bubblestress_new.npy') #bubble
hstress=np.load('hidenstress.npy') #high stress
t=dstress[:,0]*4.0

t1=np.linspace(7754,8308,555)
t1=4.0*t1

meanradial_b=bstress[:,0]-(bstress[:,1]+bstress[:,2]+bstress[:,3])
meanvertical_b=bstress[:,4]-(bstress[:,5]+bstress[:,6]+bstress[:,7])
meanradial_h=hstress[:,0]-(hstress[:,1]+hstress[:,2]+hstress[:,3])
meanvertical_h=hstress[:,4]-(hstress[:,5]+hstress[:,6]+hstress[:,7])

plt.figure(1) #paper figure 3
plt.plot(t,cstress[:,1],'r-',label='radial - corona',linewidth=2)
plt.plot(t,cstress[:,6],'r--',label='vertical - corona',linewidth=2)
plt.plot(t,dstress[:,1],'k-',label='radial - disk',linewidth=2)
plt.plot(t,dstress[:,6],'k--',label='vertical - disk',linewidth=2)
plt.title("Total Stress Comparison")
plt.xlabel(r"$t$ [$r_g/c$]",ha='center',labelpad=6,fontsize=22)
plt.ylabel(r'$\alpha$',fontsize=22)
plt.axvline(x=31016, color='k',linestyle='--')
plt.axvline(x=31164, color='k',linestyle='--')
plt.axvline(x=31744, color='k',linestyle='--')
plt.axvline(x=32800, color='k',linestyle='--')
plt.axvspan(31016,33240,facecolor='b',alpha=0.25)
plt.savefig('plotsagain/totstr_dc_paper.pdf')

